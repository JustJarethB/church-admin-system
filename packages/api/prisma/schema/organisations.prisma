model Organisations {
    id          String   @id @default(cuid())
    name        String
    description String?
    creator     Users    @relation(fields: [creatorId], references: [id])
    creatorId   String   @unique @db.Uuid
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    @@map("organisations")
    @@schema("public")
}
