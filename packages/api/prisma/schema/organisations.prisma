model Organisations {
    id          String     @id @default(uuid()) @db.Uuid
    name        String
    description String?
    creator     AppUsers   @relation(fields: [creatorId], references: [id])
    creatorId   String     @unique @db.Uuid
    createdAt   DateTime   @default(now())
    updatedAt   DateTime   @updatedAt
    Profiles    Profiles[]
    Teams       Teams[]

    @@map("organisations")
    @@schema("public")
}
